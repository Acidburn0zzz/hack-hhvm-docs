<refentry xml:id="hack.mcrouter.mcrouterexception.construct" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>MCRouterException::__construct</refname>
  <refpurpose>Construct an MCRouterException</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <constructorsynopsis>
   <modifier>public</modifier>
   <methodname>MCRouterException::__construct</methodname>
   <methodparam>
    <type>string</type>
    <parameter>message</parameter>
   </methodparam>
   <methodparam>
    <type>int</type>
    <parameter>op</parameter>
    <initializer>MCRouter::mc_op_unknown</initializer>
   </methodparam>
   <methodparam>
    <type>int</type>
    <parameter>reply</parameter>
    <initializer>MCRouter::mc_res_unknown</initializer>
   </methodparam>
   <methodparam>
    <type>string</type>
    <parameter>key</parameter>
    <initializer>""</initializer>
   </methodparam>
  </constructorsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
    <variablelist>
      <varlistentry>
        <term><parameter>message</parameter></term>
        <listitem>
          <para>
            The exception message
          </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term><parameter>op</parameter></term>
        <listitem>
          <para>
            The MCRouter operation
          </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term><parameter>reply</parameter></term>
        <listitem>
          <para>
            The MCRouter response
          </para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term><parameter>key</parameter></term>
        <listitem>
          <para>
            The key
          </para>
        </listitem>
      </varlistentry>
    </variablelist>
  </para>
 </refsect1>

</refentry>

