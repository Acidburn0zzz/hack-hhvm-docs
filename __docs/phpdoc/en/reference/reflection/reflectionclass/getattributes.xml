<refentry xml:id="reflectionclass.getattributes" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionClass::getAttributes</refname>
  <refpurpose>Gets all <link linkend="hack.attributes">attributes</link></refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>array&lt;arraykey, array&lt;int, mixed&gt;&gt;</type><methodname>ReflectionClass::getAttributes</methodname>
   <void />
  </methodsynopsis>
  <para>
   Returns all attributes.
  </para>
  &hack.only;

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns all <link linkend="hack.attributes">attributes</link>.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><methodname>ReflectionClass::getAttributes</methodname> example</title>
    <programlisting role="php">
<![CDATA[
<?hh
<<Example(1, 'Attribute'), MoreAttributes>>
class SampleClass {
}

$class = new ReflectionClass('SampleClass');
var_dump($class->getAttributes());
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
array(2) {
  ["Example"]=>
  array(2) {
    [0]=>
    int(1)
    [1]=>
    string(9) "Attribute"
  }
  ["MoreAttributes"]=>
  array(0) {
  }
}
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionClass::getAttribute</methodname></member>
    <member><methodname>ReflectionClass::getAttributesRecursive</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

